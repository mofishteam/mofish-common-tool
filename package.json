{
  "name": "mofish-common-tool",
  "version": "0.0.1",
  "description": "A toolbox for mofish plugins.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon test/index.js --watch src,test --exec \"npm run lint && babel-node --presets env --plugins babel-plugin-syntax-object-rest-spread\"",
    "build": "babel src --presets env --plugins babel-plugin-syntax-object-rest-spread --out-dir dist",
    "lint": "eslint src test",
    "test": "mocha test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mofishteam/mofish-common-tool.git"
  },
  "keywords": [
    "mofish",
    "common",
    "tool"
  ],
  "author": "wszxdhr@126.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mofishteam/mofish-common-tool/issues"
  },
  "homepage": "https://github.com/mofishteam/mofish-common-tool#readme",
  "devDependencies": {
    "@babel/polyfill": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0"
  }
}
